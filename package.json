{
  "name": "ReactNativeTestApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "clean-pod": "cd ios && rm -rf Pods && rm -rf Podfile.lock && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "android-release": "cd android && ./gradlew :app:assembleRelease && cd ..",
    "refresh-ios": "npm run clean && npm run clean-pod && npm install && npm run pod",
    "refresh-android": "npm run clean && npm install"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-video": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.19.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
